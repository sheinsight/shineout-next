{
  "name": "sheinx",
  "private": true,
  "version": "0.0.1",
  "description": "A react library developed with sheinx",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "start": "npm run dev",
    "clean:cache": "node ./scripts/cleanCache.js",
    "dev": "cross-env NODE_ENV=development CASE_ENV=test NODE_ENV=development  node ./webpack/config.dev.ts",
    "dev:create": "cross-env NODE_ENV=development CASE_ENV=test  node ./scripts/dev-create.js",
    "build:md": "cross-env NODE_ENV=development CASE_ENV=test node ./scripts/dev-doc.js",
    "build:doc": "cross-env NODE_ENV=production webpack --config ./webpack/config.doc.js",
    "build": "pnpm --filter=./packages/* run build",
    "build:watch": "pnpm --filter=./packages/* run build:watch",
    "prepare": "husky install",
    "doctor": "father doctor",
    "lint": "npm run lint:es",
    "lint:es": "eslint \"packages/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "authors": ["chenlihao@shein.com"],
  "license": "MIT",
  "files": ["dist"],
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "lint-staged": {
    "*.{md,json}": ["prettier --write --no-error-on-unmatched-pattern"],
    "*.{css,less}": ["stylelint --fix", "prettier --write"],
    "*.{js,jsx}": ["eslint --fix", "prettier --write"],
    "*.{ts,tsx}": ["eslint --fix", "prettier --parser=typescript --write"]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "immer": "10.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jest/types": "^29.5.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.4.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@umijs/lint": "^4.0.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.4",
    "ejs": "^3.1.9",
    "eslint": "^8.23.0",
    "father": "^4.1.9",
    "glob": "^10.2.5",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "markdown-it": "^13.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "react-jss": "10.9.2",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.11.2",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.3",
    "stylelint": "^14.9.1",
    "ts-deepmerge": "^1.0.0",
    "ts-loader": "^9.4.2",
    "ts-morph": "^19.0.0",
    "typescript": "^4.9.0",
    "unified": "^10.1.2",
    "valtio": "^1.10.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
  }
}
