{
  "name": "sheinx",
  "private": true,
  "version": "3.8.0-beta.16",
  "description": "A react library developed with sheinx",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.4.0",
  "scripts": {
    "start": "pnpm run dev",
    "clean:cache": "node ./scripts/cleanCache.js",
    "dev": "cross-env NODE_ENV=development CASE_ENV=test node ./webpack/config.dev.js",
    "dev:create": "cross-env NODE_ENV=development CASE_ENV=test  node ./scripts/dev-create.js",
    "build:doc": "cross-env NODE_ENV=production webpack --config ./webpack/config.doc.js",
    "build:api": "node ./scripts/api-build.js",
    "build": "pnpm run build:doc && cp .gateway-publish dist",
    "build:pkg": "node ./scripts/build-index && pnpm run build:bundless && pnpm run build:bundle",
    "build:bundless": "pnpm --filter=./packages/* run build",
    "build:bundle": "pnpm --filter=./packages/shineout run build:bundle ",
    "build:watch": "pnpm --filter=./packages/* run build:watch",
    "clean": "pnpm --filter=./packages/* run clean",
    "release": "node ./scripts/publish.js",
    "prepare": "husky install",
    "doctor": "father doctor",
    "lint": "pnpm run lint:es",
    "lint:es": "eslint \"packages/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && npm run build",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage -u",
    "test:merge": "cross-env NODE_ENV=test node ./scripts/merge-coverage.js",
    "cypress:open": "cross-env NODE_ENV=test cypress open",
    "cypress:run": "cross-env NODE_ENV=test cypress run --browser chrome"
  },
  "authors": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sheinsight/shineout-next"
  },
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "pnpm": {
    "overrides": {
      "react-dom": "18.3.1",
      "react": "18.3.1"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/runtime": "^7.23.5",
    "classnames": "2.3.2",
    "core-js": "3.33.3",
    "fflate": "^0.8.2",
    "immer": "10.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "optionalDependencies": {
    "cypress": "^13.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@cypress/code-coverage": "^3.12.10",
    "@jest/types": "^29.5.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.4.0",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@umijs/lint": "^4.0.87",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "chokidar": "^3.5.3",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.4",
    "cypress": "^13.1.0",
    "ejs": "^3.1.9",
    "eslint": "^8.52.0",
    "father": "^4.4.0",
    "glob": "^10.2.5",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "markdown-it": "^13.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "minimist": "1.2.8",
    "multer": "1.4.5-lts.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "react-jss": "10.9.2",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.11.2",
    "react-syntax-highlighter": "15.5.0",
    "react-test-renderer": "^18.2.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.3",
    "stylelint": "^14.9.1",
    "ts-deepmerge": "^1.0.0",
    "ts-loader": "^9.4.2",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "unified": "^10.1.2",
    "valtio": "^1.13.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.0"
  }
}
