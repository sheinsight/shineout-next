{
  "name": "@sheinx/shineout-mcp",
  "version": "3.7.10-beta.3",
  "description": "Shineout documentation provider for Claude AI via MCP protocol",
  "keywords": [
    "shineout",
    "claude",
    "mcp",
    "ai",
    "documentation",
    "react",
    "components"
  ],
  "author": "Shineout Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sheinsight/shineout-next.git",
    "directory": "packages/shineout-mcp"
  },
  "bugs": {
    "url": "https://github.com/sheinsight/shineout-next/issues"
  },
  "homepage": "https://github.com/sheinsight/shineout-next/tree/main/packages/shineout-mcp",
  "type": "module",
  "main": "dist/server/index.js",
  "bin": {
    "sheinx-mcp": "./bin/shineout-mcp.js"
  },
  "files": [
    "bin",
    "dist",
    "README.md",
    "INSTALLATION.md",
    "claude-config-example.json",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run update-version && tsc && npm run copy-data",
    "clean": "rm -rf dist",
    "copy-data": "mkdir -p dist/data && cp -r src/data/generated dist/data/ 2>/dev/null || echo 'No generated data to copy'",
    "dev": "tsc --watch",
    "generate": "tsx scripts/generate-data.js",
    "test": "node test-mcp.js",
    "update-version": "node scripts/update-version.js",
    "prepublishOnly": "npm run generate && npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}